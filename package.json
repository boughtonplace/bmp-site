{
  "private": true,
  "name": "bmp-site",
  "description": "Boughton Monchelsea Place website",
  "version": "0.0.1",
  "author": "Tim Kendrick <timkendrick@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run format && npm run lint && gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --trailing-comma es5 --single-quote --arrow-parens always --write \"src/**/*.{js,jsx}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": ["eslint --fix", "prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boughtonplace/bmp-site.git"
  },
  "bugs": {
    "url": "https://github.com/boughtonplace/bmp-site/issues"
  },
  "homepage": "https://github.com/boughtonplace/bmp-site#readme",
  "devDependencies": {
    "@timkendrick/eslint-config": "^1.0.0",
    "eslint": "^4.0.0",
    "gatsby": "^1.0.0",
    "gatsby-image": "^1.0.0",
    "gatsby-link": "^1.0.0",
    "gatsby-plugin-react-helmet": "^2.0.0",
    "gatsby-plugin-sharp": "^1.0.0",
    "gatsby-source-filesystem": "1.5.15",
    "gatsby-transformer-sharp": "^1.0.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.0",
    "lint-staged": "^6.0.0",
    "prettier": "^1.0.0",
    "prop-types": "^15.0.0",
    "react": "^15.0.0",
    "react-helmet": "^5.0.0"
  }
}
